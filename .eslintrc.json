{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "overrides": [
    {
      "files": ["*.jsx", "*.js"]
    }
  ],
  "plugins": ["react"],
  "rules": {
    "react/no-unescaped-entities": 0,
    "react/jsx-filename-extension": 0,
    "react/destructuring-assignment": 0
  }
}
